{
    "name": "milroyfraser/laravel-obfuscate",
    "type": "library",
    "description": "Laravel package for obfuscating ids in urls.",
    "keywords": [
        "milroyfraser",
        "laravel-obfuscate"
    ],
    "homepage": "https://github.com/milroyfraser/laravel-obfuscate",
    "license": "MIT",
    "authors": [
        {
            "name": "Milroy E. Fraser",
            "email": "milroy@outlook.com",
            "homepage": "https://github.com/milroyfraser",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "illuminate/database": "~5.6.34|~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.6.34|~5.7.0|~5.8.0|^6.0",
        "jenssegers/optimus": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelObfuscate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelObfuscate\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "LaravelObfuscate\\ObfuscateServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
